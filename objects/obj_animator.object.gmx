    alarm[2] = 300
}else if(obj_player_data.defend &gt;= 1){
    effect_create_below(ef_flare, obj_player.x, obj_player.y, 2, c_blue)
    for(i = 0; i &lt; 6; i++){
        obj_player_data.defending_spell[i] = obj_player_data.spell_cast[i]
    }
    player_ready = true
}else if(obj_player_data.focus &gt;= 1){
