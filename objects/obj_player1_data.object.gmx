        <arguments>
          <argument>
            <kind>1</kind>
            <string>var element_total = water + earth + fire + air + shadow;
var thisSpellStrength = 2 + ((element_total * (element_total - 1)) / 2);
var target = obj_player2_data;

var target_defending_spell_element_total = target.defending_spell[0];
for(i = 1; i &lt; 5; i++){
    target_defending_spell_element_total += target.defending_spell[0];
}
var defendingSpellStrength = 0;
if(target_defending_spell_element_total &gt; 0){
    defendingSpellStrength = 2 + ((target_defending_spell_element_total * 
        (target_defending_spell_element_total - 1)) / 2);
}
    
//Attacker bonuses for beating specific elements
if(water &gt;= 1){
    if(target.defending_spell[1] &gt;= 1){
        thisSpellStrength += 1
    }
}

//Defender bonuses for beating specific elements
if(target.defending_spell[0] &gt;= 1){
    if(earth &gt;= 1){
        defendingSpellStrength += 1
    }
}

var damage = thisSpellStrength - defendingSpellStrength
if (damage &gt;= 0){
    for(i = 0; i  &lt; 5; i++){
        target.cooldowns[i] += 3*target.defending_spell[i];
    }
    if (damage &gt;= 1){
        target.player_health -= damage;
        target.focus_level = 1;
    }
}
for(i = 0; i  &lt; 5; i++){
    cooldowns[i] += 5*spell_cast[i];
    if(defending_spell[i] &gt;= 1){
        defending_spell[i] -= spell_cast[i];    
    }
}
focus_level = 1;
</string>
